<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uinbdg.skirpsi.futsal">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name="uinbdg.skirpsi.futsal.Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="uinbdg.skirpsi.futsal.Activities.LoginActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.DrawerActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.MainActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.DaftarActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.ResetPasswordActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.LapanganActivity" />

        <activity android:name="uinbdg.skirpsi.futsal.Activities.KompetisiActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.EditProfilActivity"></activity>

        <activity android:name="uinbdg.skirpsi.futsal.Activities.TimActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.MyTimActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.TimDetailActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.PemainActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.JadwalActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.MyTeamActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.TimByLapangActivity" />
        <activity android:name="uinbdg.skirpsi.futsal.Activities.PertandinganActivity" />
    </application>

</manifest>